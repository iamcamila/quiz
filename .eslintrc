{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["plugin:react/recommended","airbnb", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "react/jsx-filename-extension": [
            "warn",
            {
                "extensions": [".js", ".jsx"]
            }
        ],
        "react/react-in-jsx-scope": "off",
        "comma-dangle": "off"

    }
}
